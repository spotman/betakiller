{
  "name": "spotman/betakiller",
  "description": "BetaKiller development platform based on Kohana framework",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "7.2.*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-sqlite3": "*",
    "ext-xml": "*",
    "ext-zlib": "*",
    "aidantwoods/secureheaders": "^2.0",
    "asm89/twig-cache-extension": "^1.2",
    "bramus/monolog-colored-line-formatter": "~3.0",
    "caxy/php-htmldiff": "^0.1.6",
    "doctrine/annotations": "^1.2",
    "doctrine/cache": "^1.6",
    "dragonmantank/cron-expression": "^2.0",
    "egeniq/monolog-gdpr": "^1.0",
    "graze/console-diff-renderer": "^0.6.2",
    "graze/parallel-process": "^0.8",
    "guzzlehttp/guzzle": "~6.0",
    "hansott/psr7-cookies": "^2.0",
    "imangazaliev/didom": "^1.14",
    "jakeasmith/http_build_url": "^1",
    "middlewares/negotiation": "^1.1",
    "mmamedov/page-cache": "^2.0",
    "monolog/monolog": "^1",
    "ocramius/proxy-manager": "^2.1",
    "pcelta/doctrine-cache-factory": "^0.1.0",
    "php-console/php-console": "^3.1",
    "php-di/php-di": "^6.0",
    "php-middleware/php-debug-bar": "^3.0",
    "php-middleware/request-id": "^3.1",
    "psr/log": "^1.0",
    "psr/simple-cache": "^1.0",
    "punic/punic": "^3.1",
    "ralouphie/mimey": "^2.0",
    "react/child-process": "^0.6.1",
    "roave/doctrine-simplecache": "^2.2",
    "samdark/sitemap": "^2.0",
    "spotman/davbackup": "dev-master",
    "swiftmailer/swiftmailer": "^6.1",
    "symfony/process": "^4.2",
    "symfony/yaml": "^4.0",
    "thruway/pawl-transport": "^0.5.0",
    "thunderer/shortcode": "^0.7",
    "twig/extensions": "^1.5",
    "twig/twig": "^2.6.0",
    "vlucas/phpdotenv": "^3.3",
    "voryx/thruway": "^0.5.3",
    "zendframework/zend-diactoros": "^2.0",
    "zendframework/zend-expressive": "^3.2",
    "zendframework/zend-expressive-fastroute": "^3.0",
    "zendframework/zend-expressive-helpers": "^5.1",
    "zendframework/zend-expressive-session": "^1.1",
    "zendframework/zend-httphandlerrunner": "^1.0",
    "zendframework/zend-permissions-acl": "^2.6"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.26",
    "deployer/deployer": "^6",
    "jolicode/jolinotif": "^2.0",
    "kint-php/kint": "^3.0",
    "kint-php/kint-twig": "^2.0",
    "phpstan/phpstan": "0.10.3",
    "phpunit/php-code-coverage": "^6.0",
    "phpunit/phpunit": "^7.3",
    "roave/security-advisories": "dev-master"
  },
  "suggest": {
    "franzl/studio": "Tool for developing Composer packages"
  },
  "scripts": {
    "ci": [
      "@qa",
      "@test"
    ],
    "qa": [
      "@testEnv",
      "@phpstan"
    ],
    "test": [
      "@testEnv",
      "@phpunit"
    ],
    "testEnv": "ln -sf ./../.env.testing ./application/.env",
    "phpunit": "APP_ENV=testing ./vendor/bin/phpunit --colors=always",
    "phpstan": "APP_ENV=testing ./vendor/bin/phpstan analyze"
  },
  "config": {
    "platform": {
      "php": "7.2.15"
    },
    "sort-packages": true
  }
}

{
  "name": "spotman/betakiller",
  "description": "BetaKiller development platform based on Kohana framework",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "7.2.*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-sqlite3": "*",
    "ext-xml": "*",
    "aidantwoods/secureheaders": "^2.0",
    "asm89/twig-cache-extension": "^1.2",
    "bramus/monolog-colored-line-formatter": "~2.0",
    "caxy/php-htmldiff": "^0.1.6",
    "doctrine/annotations": "^1.2",
    "doctrine/cache": "^1.6",
    "dragonmantank/cron-expression": "^2.0",
    "graze/parallel-process": "^0.7.0",
    "guzzlehttp/guzzle": "~6.0",
    "imangazaliev/didom": "^1.10",
    "jakeasmith/http_build_url": "^1",
    "jmikola/wildcard-event-dispatcher": "~1.0",
    "mmamedov/page-cache": "^2.0",
    "monolog/monolog": "^1",
    "ocramius/proxy-manager": "^2.1",
    "pcelta/doctrine-cache-factory": "^0.1.0",
    "php-console/php-console": "^3.1",
    "php-di/php-di": "^6.0",
    "psr/log": "^1.0",
    "psr/simple-cache": "^1.0",
    "ralouphie/mimey": "^2.0",
    "roave/doctrine-simplecache": "^2.1",
    "samdark/sitemap": "^2.0",
    "spotman/davbackup": "dev-master",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/yaml": "^4.0",
    "thruway/pawl-transport": "^0.5.0",
    "thunderer/shortcode": "^0.6",
    "twig/extensions": "^1.5",
    "twig/twig": "^2.0",
    "vlucas/phpdotenv": "^2.4",
    "voryx/thruway": "^0.5.3",
    "zendframework/zend-diactoros": "^1.8",
    "zendframework/zend-permissions-acl": "^2.6"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.20",
    "deployer/deployer": "^6",
    "phpstan/phpstan": "^0.10.3",
    "phpunit/dbunit": "^4.0",
    "phpunit/php-code-coverage": "^6.0",
    "phpunit/phpunit": "^7.3",
    "roave/security-advisories": "dev-master"
  },
  "suggest": {
    "franzl/studio": "Tool for developing Composer packages"
  },
  "scripts": {
    "ci": [
      "@qa",
      "@test"
    ],
    "qa": [
      "@testEnv",
      "@phpstan"
    ],
    "test": [
      "@testEnv",
      "@phpunit"
    ],
    "testEnv": "ln -sf ./../.env.testing ./application/.env",
    "phpunit": "APP_ENV=testing phpunit --colors=always",
    "phpstan": "APP_ENV=testing phpstan analyze"
  },
  "config": {
    "platform": {
      "php": "7.2.9"
    },
    "sort-packages": true
  }
}

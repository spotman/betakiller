{% extends layout %}

{% block page_content %}

  <div class="row">
    <div class="col s12">
      {#<a href="{{ createUrl }}" class="btn right">Create post</a>#}
    </div>
  </div>

  <div class="row">
    <div class="col s12">
      <ul class="collection">
        {% for revision in revisions %}
          <li class="collection-item">
            {{ revision.created_at }}
            {{ revision.created_by }}
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>

{% endblock %}

{% block doctype %}<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">{% endblock %}

{% block html_tag %}<html>{% endblock %}

    <head>
        {{ meta.content_type("text/html") }}
        <title>{{ meta.title|join(" - ") }}</title>
        {% block head %}
        {{ meta.render|raw }}
        {{ css.get_all|raw }}
        {{ js.get_all|raw }}
        {% endblock head %}
    </head>
    <body>
    {{ content|raw }}
    </body>
</html>